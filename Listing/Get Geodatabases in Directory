#Using Directory Method

import arcpy
import os
dir = r"C:\arcgis\ArcTutor\AAA\GDBs"
GDBfiles =[]
for file in os.listdir(dir):
    if file.endswith(".gdb"):
       GDBfiles.append(file)
       print(GDBfiles)


#=====================================================================
#Using Arcpy Workspace Method

import arcpy
import os
ws = "C:\arcgis\ArcTutor\AAA\GDBs"
GDBfiles =[]
for file in os.listdir(ws):
    if file.endswith(".gdb"):
       GDBfiles.append(file)
       print(GDBfiles)
